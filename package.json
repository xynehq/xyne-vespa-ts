{
  "name": "@xyne/vespa-ts",
  "version": "1.0.12",
  "publishConfig": {
    "access": "public"
  },
  "description": "A reusable TypeScript package for interacting with Vespa search engine with dependency injection support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "type": "module",
  "private": false,
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.esm.js",
      "require": "./dist/types.cjs",
      "types": "./dist/src/types.d.ts"
    },
    "./mappers": {
      "import": "./dist/mappers/index.esm.js",
      "require": "./dist/mappers/index.cjs",
      "types": "./dist/src/mappers/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.esm.js",
      "require": "./dist/utils/index.cjs",
      "types": "./dist/src/utils/index.d.ts"
    },
    "./errors": {
      "import": "./dist/errors/index.esm.js",
      "require": "./dist/errors/index.cjs",
      "types": "./dist/src/errors/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.esm.js",
      "require": "./dist/client/index.cjs",
      "types": "./dist/src/client/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsc && rollup -c",
    "dev": "run-p dev:tsc dev:rollup",
    "dev:tsc": "tsc --watch",
    "dev:rollup": "rollup -c --watch --no-watch.clearScreen",
    "dev:watch": "nodemon --watch src --watch index.ts --ext ts --exec \"bun run build\"",
    "format": "bunx biome format --write .",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vespa",
    "search",
    "typescript",
    "dependency-injection",
    "search-engine"
  ],
  "author": "Junaid Shirur",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xynehq/xyne-vespa-ts.git"
  },
  "homepage": "https://github.com/xynehq/xyne-vespa-ts#readme",
  "bugs": {
    "url": "https://github.com/xynehq/xyne-vespa-ts/issues"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/bun": "latest",
    "@types/jest": "^29.5.0",
    "chokidar-cli": "^3.0.0",
    "jest": "^29.5.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.46.2",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "p-limit": "^6.2.0",
    "zod": "^4.0.15"
  }
}
